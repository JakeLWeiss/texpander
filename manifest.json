{
  "manifest_version": 2,
  "name": "Expando",
  "version": "0.1",
  
  "web_accessible_resources": [
    "content.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/*","http://www.reddit.com/*"
      ],
      "js": ["jquery-3.2.1.js", "content.js"]
    }
  ],
  "browser_action": {
	"default_icon": "icon.png"
	}
}