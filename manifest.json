{
  "manifest_version": 2,
  "name": "Expando",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "content.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/r/*","http://www.reddit.com/r/*"
      ],
      "js": ["jquery-3.2.1.js", "content.js"]
    }
  ],
  "browser_action": {
    
  }
}